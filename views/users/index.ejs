<%- include('../partials/header') %>
<h3 class="title">User List Page</h3>
<% users.forEach(u => { %>
<a class="links" href="/users/<%= u._id %>">
  <div class="card">
    <div>
      <img height="50" id="avatarPhoto" src="<%= u.avatar ? u.avatar : 'http://theoldreader.com/kittens/300/300/' %>">
      <%= u.name %>
</a>
</div>
</div><br>
<% }) %>

<%- include('../partials/footer') %>